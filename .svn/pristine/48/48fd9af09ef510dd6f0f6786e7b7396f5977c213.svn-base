<Ui>
  <Script file="GH_UI.lua"/>
  <!-- GH UI Templates -->
  <Button name = "GHButtonTemplate" virtual="true" frameStrata="HIGH" inherits="UIPanelButtonTemplate">
    <Size>
      <AbsDimension x="120" y="20" />
    </Size>
  </Button>

  <CheckButton name = "GHCheckBoxTemplate" virtual="true" frameStrata="HIGH" inherits="InterfaceOptionsCheckButtonTemplate">
    <Scripts>
      <OnLoad>
        GHCheckBox_OnLoad(self)
			</OnLoad>
      <OnClick>
        GHCheckBox_OnClick(self, event, arg1)
      </OnClick>
      <OnShow>
        GHCheckBox_OnShow(self)
      </OnShow>
		</Scripts>
  </CheckButton>

  <EditBox name = "GHEditBoxTemplate" virtual="true" frameStrata="HIGH" inherits="UICheckButtonTemplate">
    <Size>
      <AbsDimension x="120" y="20" />
    </Size>
  </EditBox>
  <!-- End Templates -->

  <!-- GH UI Items -->
  <Frame name="GearHelper" enableMouse="true" parent="InterfaceOptionsFramePanelContainer">
    <Scripts>
      <OnLoad>
        self:RegisterEvent("PLAYER_LOGIN")
        MainOptionFrame_OnLoad(self)
			</OnLoad>
      <OnEvent>
        GHOnEvent(event, ...)
      </OnEvent>
      <OnShow>
        MainOptionFrame_OnShow(self)
      </OnShow>
		</Scripts>
    <Frames>
      <CheckButton name="Addon" inherits="GHCheckBoxTemplate">
        <Anchors>
          <Anchor point="CENTER">
            <Offset>
              <AbsDimension x="0" y="0"/>
            </Offset>
          </Anchor>
        </Anchors>
      </CheckButton>
      <FontString inherits="GameFontNormalLarge" frameStrata="ARTWORK" text="GearHelper">
        <Size x="100" y="100"/>
        <Anchors>
          <Anchor point="TOP" />
        </Anchors>
        <Scripts>
          <OnLoad>
            self:SetText("GearHelper")
          </OnLoad>
        </Scripts>
      </FontString>
    </Frames>
  </Frame>

  <Frame name="Custom Weights" enableMouse="true" parent="InterfaceOptionsFramePanelContainer">
    <Scripts>
      <OnLoad>
        CustomWeightsFrame_OnLoad(self);
			</OnLoad>
      <OnShow>
        CustomWeightsFrame_OnShow(self);
      </OnShow>
		</Scripts>
  </Frame>
