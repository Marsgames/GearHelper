 <Ui xmlns="http://www.blizzard.com/wow/ui/"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <Frame name="GH_MainFrame" parent="UIParent" toplevel="true" hidden="true" enableMouse="true">
     <Size>
       <AbsDimension x="500" y="500" />
     </Size>
     <Anchors>
       <Anchor point="CENTER" />
     </Anchors>
     <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background"
           edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
       <BackgroundInsets>
         <AbsInset left="11" right="12" top="12" bottom="11"/>
       </BackgroundInsets>
       <TileSize>
         <AbsValue val="32"/>
       </TileSize>
       <EdgeSize>
         <AbsValue val="32"/>
       </EdgeSize>
     </Backdrop>
     <Frames>
       <Button name="closeupepe" parentKey="close" inherits="UIPanelCloseButton">
         <Anchors>
           <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT">
           </Anchor>
         </Anchors>
       </Button>
     </Frames>
 <Frames>
                <CheckButton name="$parentCBOn" inherits="OptionsCheckButtonTemplate">
                    <scripts>
                        <OnClick>
                            
                        </OnClick>
                    </scripts>
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parent">
                                <Offset><AbsDimension x="20" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBGrey" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBOn">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBCupi" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBGrey">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBQuest" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBCupi">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBNeed" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBQuest">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBEquipPick" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBNeed">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
                <CheckButton name="$parentCBEquipSpe" inherits="OptionsCheckButtonTemplate">
                        <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="$parentCBEquipPick">
                                <Offset><AbsDimension x="0" y="-30"/></Offset>
                                </Anchor>
                        </Anchors>
                </CheckButton>
</Frames>


     <Scripts>
      <OnLoad>
        GHUI_OnLoad(self)
      </OnLoad>
     </Scripts>
   </Frame>
   <Button name="MyMod_MinimapButton" parent="Minimap" enableMouse="true" movable="true" hidden="false">
                <Size>
                        <AbsDimension x="33" y="33"/>
                </Size>
                <Anchors>
                        <Anchor point="BOTTOMLEFT">
                                <Offset>
                                    <!--   x="-14" y="7"   -->
                                        <AbsDimension x="-14" y="7"/>
                                </Offset>
                        </Anchor>
                </Anchors>
                <Layers>
                        <Layer level="BACKGROUND">
                                <Texture name="MyMod_MinimapButton_Icon" file="Interface\AddOns\MyMod\Icons\minimap.blp">
                                        <Size>
                                                <AbsDimension x="21" y="21"/>
                                        </Size>
                                        <Anchors>
                                                <Anchor point="TOPLEFT">
                                                        <Offset>
                                                                <AbsDimension x="7" y="-6"/>
                                                        </Offset>
                                                </Anchor>
                                        </Anchors>
                                </Texture>
                        </Layer>
                        <Layer level="OVERLAY">
                                <Texture file="Interface\Minimap\MiniMap-TrackingBorder">
                                        <Size>
                                                <AbsDimension x="56" y="56"/>
                                        </Size>
                                        <Anchors>
                                                <Anchor point="TOPLEFT"/>
                                        </Anchors>
                                </Texture>
                        </Layer>
                </Layers>
                <Frames>
                        <Frame name="$parentTooltip" parentKey="tooltip" enableMouse="true" setAllPoints="true" hidden="true"/>
                        <Frame name="MyMod_MinimapButton_DraggingFrame" hidden="true">
                                <Scripts>
                                        <OnUpdate>
                                                onMinimapButtonUpdate()
                                        </OnUpdate>
                                </Scripts>
                        </Frame>
                </Frames>
                <HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
                <Scripts>
                        <OnLoad>
                                self:RegisterForClicks("LeftButtonUp","RightButtonUp")
                                self:RegisterForDrag("LeftButton","RightButton")
                        </OnLoad>
                        <OnDragStart>
                                self.dragging = true
                                GameTooltip:Hide()
                                self:LockHighlight()
                                MyMod_MinimapButton_DraggingFrame:Show()
                        </OnDragStart>
                        <OnDragStop>
                                self.dragging = false
                                self:UnlockHighlight()
                                MyMod_MinimapButton_DraggingFrame:Hide()
                        </OnDragStop>
                        <OnClick>
                                onMinimapButtonClick(button)
                        </OnClick>
                </Scripts>
        </Button>
 </Ui>